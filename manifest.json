{
  "manifest_version": 2,

  "name": "Tab Overflow",
  "description": "This is an extension for managin navigation with a large number by 'overflowing' excess tabs into an index tab.",
  "version": "1.0",
  "background": {
    "pesistent": "false",
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs",
    "history"
  ],
  "content_scripts": [{
      "matches": ["http://*/*", "https://*/*"],
      "js": ["num_script.js"]
    },{
      "matches": ["http://*/*", "https://*/*"],
      "js": ["overflow_tab.js"]
    }
    ]
}
